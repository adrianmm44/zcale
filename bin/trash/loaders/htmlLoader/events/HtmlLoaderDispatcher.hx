package hxf.core.loaders.htmlLoader.events;

import hxf.core.remote.events.RemoteDispatcher;
import hxf.core.loaders.htmlLoader.events.HtmlLoaderData;

class HtmlLoaderDispatcher extends RemoteDispatcher
{
	/////////////////////////////////////////////////////////////////////////////////////////////////////
	
	private var evtDataHtmlLoader : HtmlLoaderData;
	
	/////////////////////////////////////////////////////////////////////////////////////////////////////
	
	public function new( sub : Dynamic, reg : Dynamic )
	{
		super( sub, reg );
		
		evtDataHtmlLoader = new HtmlLoaderData();
	}
	
	/////////////////////////////////////////////////////////////////////////////////////////////////////
	
	override public function eventData() : HtmlLoaderData
	{
		return evtDataHtmlLoader;
	}
	
	/////////////////////////////////////////////////////////////////////////////////////////////////////
}

