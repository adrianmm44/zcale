package hxf.core.dom.element.style.properties.background.targets;

import hxf.core.dom.element.targets.FlashElement;

class FlashBackground
{
	/////////////////////////////////////////////////////////////////////////////////////////////////////
	
	public var color( default, set ) : String;
	
	private var element : FlashElement;
	
	/////////////////////////////////////////////////////////////////////////////////////////////////////
	
	public function new( element : FlashElement )
	{
		this.element = element;
	}
	
	/////////////////////////////////////////////////////////////////////////////////////////////////////
	
	private function set_color( value : String ) : String
	{
		return color = value;
	}
	
	/////////////////////////////////////////////////////////////////////////////////////////////////////
}

